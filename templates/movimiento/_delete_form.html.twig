<form method="post" action="{{ path('movimiento_delete', {'idMov': movimiento.idMov}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ movimiento.idMov) }}">
    <button class="btn">Delete</button>
</form>
